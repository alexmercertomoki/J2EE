package array;

public class ConfirmRotateArray {
	
	public boolean rotateArr(String p, String s) {
		
		int j = 0;
		char begin = s.charAt(j);
	//	boolean flag = true; 
		
		for(int i = 0; i < p.length(); i++) {
			if(begin == p.charAt(i)) {
				while(i < p.length() && s.charAt(j) == p.charAt(i)) {
					i++;
					j++;
				}
				if(i == p.length()) {
					break;
				}
				return false;
			}
		}
		
		int i = 0; 
		for( ;j < s.length(); j++) {
			if(s.charAt(j) == p.charAt(i)) {
				
			}
			
		}
		
		
		
	}

}
